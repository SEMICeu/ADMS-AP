  <rdf:Description rdf:about="http://purl.org/adms/sw/effort">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <dcterms:identifier xml:lang="en">admssw:effort</dcterms:identifier>
