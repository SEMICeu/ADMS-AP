<!-- Software Project -->

  <rdf:Description rdf:about="http://purl.org/adms/sw/SoftwareProject">
    <dcterms:identifier xml:lang="en">admssw:SoftwareProject</dcterms:identifier>
    <rdfs:subClassOf rdf:resource="http://usefulinc.com/ns/doap#Project"/>
    <rdfs:label xml:lang="en">Software Project</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://joinup.ec.europa.eu/asset/adms_foss/release/release100"/>
    <rdfs:comment xml:lang="en">A Software Project is a time-delimited undertaking with the objective to produce one or more software releases, materialised as software packages. Some projects are long-running undertakings, and do not have a clear time-delimited nature or project organisation. In this case, the term 'software project' can be interpreted as the result of the work: a collection of related software releases that serve a common purpose.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://usefulinc.com/ns/doap#description">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/description"/>
    <dcterms:identifier xml:lang="en">doap:description</dcterms:identifier>
    <rdfs:label xml:lang="en">Software Project - description</rdfs:label>
    <rdfs:comment xml:lang="en">Both the dcterms:description and doap:description properties fully cover the ADMS.SW property of the same name. </rdfs:comment>
  </rdf:Description>

  <rdf:Description rdf:about="http://usefulinc.com/ns/doap#homepage">
